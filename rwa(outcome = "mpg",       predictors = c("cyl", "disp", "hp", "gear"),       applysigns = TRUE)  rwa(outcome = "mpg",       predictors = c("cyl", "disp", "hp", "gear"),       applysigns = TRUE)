      library(rwa)
mtcars <- read.csv("https://raw.githubusercontent.com/Awesin/PopDat/main/Religion.csv")
mtcars %>%
rwa(outcome = "mpg",
predictors = c("protestant", "catholic", "jehovah", "orthodox", "mormon", "unaffiliated", "jewish"),
applysigns = TRUE)
 
 mtcars %>%
  rwa(outcome = "mpg",
      predictors = c("protestant", "catholic", "jehovah", "orthodox", "mormon", "unaffiliated", "jewish"),
      applysigns = TRUE)
      
